package com.flight.assignment;

import java.util.List;

public class Route {

	 private final List<City> vertexes;
	  private final List<Flight> edges;

	  public Route(List<City> vertexes, List<Flight> edges) {
	    this.vertexes = vertexes;
	    this.edges = edges;
	  }

	  public List<City> getVertexes() {
	    return vertexes;
	  }

	  public List<Flight> getEdges() {
	    return edges;
	  }
	  
	  
}
